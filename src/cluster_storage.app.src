{application, cluster_storage,
 [
  {description, "Cluster-wide storage for storing primitive data on cluster nodes."},
  {vsn, "1.0.0"},
  {registered, []},
  {applications, [
                  kernel,
                  stdlib
                 ]},
  {mod, {cluster_storage_app, []}},
  {env, [
    {storage_nodes, [sname1@node1]},
    {buckets, [cs_bucket1]},
    {dump_dir, "/var/log"}
  ]}
 ]}.
